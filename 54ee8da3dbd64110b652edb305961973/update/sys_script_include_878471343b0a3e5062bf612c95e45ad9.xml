    <sys_script_include action="INSERT_OR_UPDATE">
        <access>package_private</access>
        <active>true</active>
        <api_name>x_snc_airline.QueryAirportsForMap</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Returns a GlideRecord object representing the Airports to be displayed on the Airport map.</description>
        <mobile_callable>false</mobile_callable>
        <name>QueryAirportsForMap</name>
        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[function QueryAirportsForMap()
{
	var grAirports = new GlideRecord('x_snc_airline_airport');
	grAirports.addQuery('longitude','!=','');
	grAirports.addQuery('latitude','!=','');
	grAirports.query();
	return grAirports;
}]]></script>
